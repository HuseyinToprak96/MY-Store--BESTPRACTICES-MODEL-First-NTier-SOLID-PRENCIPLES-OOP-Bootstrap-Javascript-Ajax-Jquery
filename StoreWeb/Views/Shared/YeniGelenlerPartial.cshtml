@model List<CoreLayer.Entities.Urun>

<div id="yeniler">
    <h2>YENİ GELEN ÜRÜNLER</h2>
    @foreach (var item in Model)
    {
       
        <div id="yeniUrun_@item.Id" class="yeni"><img src="@item.Resim" height="150" width="150"/>
            <div>
                <a href="/Urun/Detay/id=@item.Id"> @item.UrunAdi</a> <div class="ucret"> @item.Ucret TL</div>
            </div>
    </div>
    }
</div>
<style>
    #yeniler{
        margin-top:50px;
    }
</style>